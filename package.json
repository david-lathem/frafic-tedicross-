    "cli-highlight": "2.1.11",
    "discord.js": "^14.18.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "js-yaml": "4.1.0",
    "mime": "3.0.0",
    "moment": "2.30.1",
    "nodemon": "3.1.7",
    "prettier": "3.3.3",
